      "source": [
        "import numpy as np\n",
        "import matplotlib.pyplot as plt\n",
        "from ipywidgets import interact, FloatSlider, IntSlider\n",
        "\n",
        "# Función para generar y graficar el patrón de interferencia\n",
        "def interferencia_biprisma(wavelength, d, D, screen_width, num_points):\n",
        "    # Calcula las posiciones en la pantalla\n",
        "    x = np.linspace(-screen_width / 2, screen_width / 2, num_points)\n",
        "\n",
        "    # Diferencia de camino óptico\n",
        "    path_difference = (x * d) / D\n",
        "\n",
        "    # Intensidad en cada punto\n",
        "    intensity = (np.cos(2 * np.pi * path_difference / wavelength))**2\n",
        "\n",
        "    # Normalizar la intensidad\n",
        "    intensity /= np.max(intensity)\n",
        "\n",
        "    # Graficar el patrón de interferencia\n",
        "    plt.figure(figsize=(10, 4))\n",
        "    plt.plot(x * 1e3, intensity, 'b-')\n",
        "    plt.title('Patrón de Interferencia del Biprisma de Fresnel')\n",
        "    plt.xlabel('Posición en la pantalla (mm)')\n",
        "    plt.ylabel('Intensidad Normalizada')\n",
        "    plt.grid(True)\n",
        "    plt.show()\n",
        "\n",
        "# Parámetros iniciales\n",
        "wavelength_init = 500e-9\n",
        "d_init = 0.5e-3\n",
        "D_init = 1\n",
        "screen_width_init = 0.01\n",
        "num_points_init = 5000\n",
        "\n",
        "# Crear sliders interactivos\n",
        "interact(interferencia_biprisma,\n",
        "         wavelength=FloatSlider(value=wavelength_init, min=300e-9, max=700e-9, step=200e-9, description='Longitud onda'),\n",
        "         d=FloatSlider(value=d_init, min=0.05e-3, max=1e-3, step=0.05e-3, description='Separación (d)'),\n",
        "         D=FloatSlider(value=D_init, min=0.1, max=2, step=0.1, description='Distancia (D)'),\n",
        "         screen_width=FloatSlider(value=screen_width_init, min=0.005, max=0.02, step=0.001, description='Ancho de la Pantalla'),\n",
        "         num_points=IntSlider(value=num_points_init, min=1, max=1000, step=1, description='Número de Puntos'))\n",
        "\n"
      ]
    },
